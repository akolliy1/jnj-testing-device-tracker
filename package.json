{
  "name": "jnj-testing-device-tracker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && env-cmd -f .env node --max_old_space_size=8192 --optimize_for_size --stack_size=4096 ./bin/www",
    "devstart": "nodemon --exec babel-node ./bin/www-dev",
    "build": "babel ./src/ -d ./dist",
    "test": "jest",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run devstart\" \"npm run client\""
  },
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "@hapi/joi": "^17.1.1",
    "bcryptjs": "^2.4.3",
    "cli-table": "^0.3.6",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^9.0.2",
    "env-cmd": "^10.1.0",
    "express": "~4.16.1",
    "express-list-endpoints": "^5.0.0",
    "express-validator": "^6.11.1",
    "http-errors": "^1.6.3",
    "http-status": "^1.5.0",
    "idempotent-babel-polyfill": "^7.4.4",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "path": "^0.12.7",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^6.1.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0"
  }
}
